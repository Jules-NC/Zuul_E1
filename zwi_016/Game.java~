import pkg_engine.*;/** *  *  C'est la main classe, ya presque rien d'autre à comprendre *  Créer une instance de la classe pour démarrer le jeu *  *  LA CLASSE PRINCIPALE QUE CA VA FAIRE LE JEU *  * @author  Jules Neghnagh -- Chenavas * @version PROTO SPAGHETTOI */public class Game{    private UserInterface gui;    private GameEngine aEngine;    /**     * Create the game and initialise its internal map.     */    public Game()     {        this.aEngine = new GameEngine();        gui = new UserInterface(this.aEngine);        this.aEngine.setGUI(gui);    }        public static void main(String[] args){        new Game();    }}